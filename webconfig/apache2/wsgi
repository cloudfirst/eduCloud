WSGIPythonPath    /usr/local/www/luhyaServer
<VirtualHost *:80>
    LogLevel info
    ServerName www.example.com
    ServerAlias example.com
    ServerAdmin webmaster@example.com

    DocumentRoot /usr/local/www/documents

    <Directory /usr/local/www/documents>
        Order allow,deny
        Allow from all
    </Directory>

    Alias /static/luhyaAdmin/   /usr/local/www/luhyaServer/luhyaAdmin/static/luhyaAdmin/
    Alias /static/admin         /usr/local/lib/python2.7/dist-packages/django/contrib/admin/static/admin

    AddType text/cache-manifest .manifest
    # ExpiresActive On
    # ExpiresByType text/cache-manifest "access plus 0 seconds"
	
    WSGIDaemonProcess example.com processes=2 threads=15 display-name=%{GROUP} python-path=/usr/local/www/luhyaServer
    WSGIProcessGroup example.com
    WSGIScriptAlias / /usr/local/www/luhyaServer/luhyaServer/wsgi.py 

    <Directory /usr/local/www/luhyaServer>
    <Files wsgi.py>
        Order deny,allow
        Allow from all
    </Files>
    </Directory>
</VirtualHost>
