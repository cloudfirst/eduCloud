WSGIPythonPath    /usr/local/www/
<VirtualHost *:80>
    LogLevel info
    ServerName www.educloud.com
    ServerAlias educloud.com
    ServerAdmin webmaster@educloud.com

    DocumentRoot /usr/local/www/documents

    <Directory /usr/local/www/documents>
        Order allow,deny
        Allow from all
    </Directory>

    Alias /static/portal/   /usr/local/www/portal/static/portal/
    Alias /static/clc/      /usr/local/www/clc/static/clc/
    Alias /static/admin     /usr/local/lib/python2.7/dist-packages/django/contrib/admin/static/admin

    AddType text/cache-manifest .manifest
    # ExpiresActive On
    # ExpiresByType text/cache-manifest "access plus 0 seconds"
	
    WSGIDaemonProcess educloud.com processes=2 threads=15 display-name=%{GROUP} python-path=/usr/local/www/
    WSGIProcessGroup educloud.com
    WSGIScriptAlias / /usr/local/www/educloud/wsgi.py

    <Directory /usr/local/www/>
    <Files wsgi.py>
        Order deny,allow
        Allow from all
    </Files>
    </Directory>
</VirtualHost>
